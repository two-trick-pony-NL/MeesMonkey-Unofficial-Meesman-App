{
  "expo": {
    "name": "MeesMonkey",
    "slug": "MeesMonkey",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/MeesmanIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow MeesMonkey to use Face ID."
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/MeesmanSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#384956"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petervandoorn.MeesMonkey",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow MeesMonkey to use Face ID."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#384956"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.petervandoorn.MeesMonkey"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bd574d3f-1d23-4374-af3c-feb4a65faec6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bd574d3f-1d23-4374-af3c-feb4a65faec6"
    }
  }
}
