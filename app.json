{
  "expo": {
    
    "name": "MeesmanUnofficial",
    "slug": "MeesmanUnofficial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/MeesmanIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Meesman to use Face ID."
        }
      ]
    ],
    "splash": {
      "image": "./assets/MeesmanSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#384956"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petervandoorn.MeesmanUnofficial"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#384956"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
