{
  "expo": {
    "name": "MeesMonkey",
    "slug": "MeesMonkey",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/MeesmanIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow MeesMonkey to use Face ID."
        }
      ]
    ],
    "splash": {
      "image": "./assets/MeesmanSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#384956"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petervandoorn.MeesMonkey",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow MeesMonkey to use Face ID."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#384956"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fcfe7e39-bc7d-4b2e-9d67-70943b59e4d6"
      }
    }
  }
}
